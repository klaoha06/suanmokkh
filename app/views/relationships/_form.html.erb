<%= simple_form_for(@relationship) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :retreat_talk_id %>
    <%= f.input :related_retreat_talk_id %>
    <%= f.input :create %>
    <%= f.input :destroy %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
